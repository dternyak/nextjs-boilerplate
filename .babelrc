{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    ["import", { "libraryName": "antd", "style": false }],
    [
      "module-resolver",
      {
        "root": ["src"],
        "extensions": [".js", ".tsx", ".ts"]
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ]
}
